$("#reset_game").removeClass("disabled");

// Reset board
$(".cell").removeClass("marked_x").removeClass("marked_o");

// Reset score
$(".score").text(0);

// Hide modal
$("#myModal").modal("hide");

// Print out status field text.
$("#status_field").text("Okay, lets play! <%= @player1_name %> is first out.")

// Print out chosen names.
$("#player1_name").text("<%= @player1_name %>");
$("#player2_name").text("<%= @player2_name %>");